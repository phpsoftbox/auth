{
    "name": "phpsoftbox/auth",
    "description": "Auth component for the PhpSoftBox framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton K.",
            "email": "inspector-who@mail.ru"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^8.4",
        "psr/simple-cache": "^3.0",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^2.0",
        "phpsoftbox/session": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.93",
        "phpsoftbox/cli-app": "dev-master",
        "phpsoftbox/cs-fixer": "^1.0.3",
        "phpsoftbox/database": "dev-master",
        "phpsoftbox/http-message": "dev-master",
        "phpsoftbox/orm": "dev-master",
        "phpunit/phpunit": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "PhpSoftBox\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpSoftBox\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "psb": {
            "providers": [
                "PhpSoftBox\\Auth\\Cli\\AuthCommandProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs:fix": "vendor/bin/psb-cs-fixer fix",
        "cs:check": "vendor/bin/psb-cs-fixer check --diff"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
